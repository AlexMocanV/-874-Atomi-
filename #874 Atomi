#include <bits/stdc++.h>
std::stack<std::pair<int,int>>s;
std::string n;
std::pair<int,int> numar(std::string n,int i)
{
    int p=10,ans=0;
    while(int(n[i])<=57&&int(n[i])>=48)
    {
        ans*=p;
        ans+=(int(n[i])-48);
        i++;
    }
    return std::make_pair(ans,i--);
}
int main()
{
    int nra=0,nrb=0;
    int a,b;
    getline(std::cin,n);
    for(int i=0;i<n.length();i++)
    {
        if(n[i]=='(')
        s.push(std::make_pair(0,0));
        else if(n[i]==')')
        {
            a=s.top().first,b=s.top().second;
            s.pop();
            if(i+1<n.length()&&int(n[i+1])<=57&&int(n[i+1])>=48)
            {
                std::pair<int,int>r=numar(n,i+1);
                a*=r.first;
                b*=r.first;
                i=r.second-1;
            }
            if(s.empty()==true){nra+=a;nrb+=b;}
            else
            {
                s.top().first+=a;
                s.top().second+=b;
            }
        }
        else if(s.empty()==true)
        {
            if(n[i]=='A')
            {
                if(i+1<n.length()&&int(n[i+1])<=57&&int(n[i+1])>=48){
                    std::pair<int,int>r=numar(n,i+1);
                    nra+=r.first;
                    i=r.second-1;
                }
                else
                {
                   nra++; 
                }
            }
            else if(n[i]=='B')
            {
                if(i+1<n.length()&&int(n[i+1])<=57&&int(n[i+1])>=48){
                    std::pair<int,int>r=numar(n,i+1);
                    nrb+=r.first;
                    i=r.second-1;
                }
                else
                {
                    nrb++;
                }
            }
        }
        else
        {
            if(n[i]=='A')
            {
                if(i+1<n.length()&&int(n[i+1])<=57&&int(n[i+1])>=48){
                    std::pair<int,int>r=numar(n,i+1);
                    s.top().first+=r.first;
                    i=r.second-1;
                }
                else s.top().first++;
            }
            else if(n[i]=='B')
            {
                if(i+1<n.length()&&int(n[i+1])<=57&&int(n[i+1])>=48){
                    std::pair<int,int>r=numar(n,i+1);
                    s.top().second+=r.first;
                    i=r.second-1;
                }
                else
                {
                    s.top().second++;
                }
            }
        }
        
    }
    std::cout<<nra<<' '<<nrb;
    return 0;
}
